queryGroup 'Stat Model'

	annotation PBI_QueryGroupOrder = 1

annotation __PBI_TimeIntelligenceEnabled = 1

annotation PBI_ProTooling = ["DevMode"]

annotation PBI_QueryOrder = ["credit_risk_data credit_risk_dataset","fact_records","dim_client","feature_importance","dim_residential_status","dim_purpose","dim_grade","dim_status"]

ref table fact_records
ref table dim_client
ref table dim_residential_status
