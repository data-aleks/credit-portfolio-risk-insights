model Model
	culture: en-GB
	defaultPowerBIDataSourceVersion: powerBI_V3
	sourceQueryCulture: en-GB
	dataAccessOptions
		fastCombine
		legacyRedirects
		returnErrorValuesAsNull

/// Original Data
queryGroup Original

	annotation PBI_QueryGroupOrder = 0

